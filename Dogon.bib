@article{Plungian1989,
	Abstract = {The grammatical status of resultative verbal structures is examined
	to determine whether the resultativity/nonresultativity opposition
	can be grammaticalized. The possibility that such a grammaticalization
	may be reflected in a verb's semantic content is explored & answered
	in the affirmative. Literature & definitions of the resultative are
	surveyed. The thesis that the resultative may describe only natural
	results is contested & supported with evidence from Dogon (Niger
	- Kongo), where resultatives do not obligatorily describe natural
	results, & yet the resultative/nonresultative opposition has a grammatical
	character. 20 References. Z. Dubiel},
	Author = {Plungian, V. A.},
	Date-Modified = {2014-10-30 14:19:18 +0000},
	Journal = {Voprosy yazykoznaniya},
	Number = {6},
