@incollection{Culy1990,
	Abstract = {In internally headed relative clauses (IHRCs), the noun being modified
	is not extracted but remains in situ. It is shown that in some languages
	with IHRCs, the form of the highest verb in a relative clause depends
	on the grammatical function of the head of the relative clause. A
	means of capturing this generalization in a head - driven phrase
	structure grammar (HPSG) is described & the framework is shown to
	predict the importance of the subject /nonsubject distinction in
	these clauses. Data from Quechua, Fula, Dogon, & other languages
	are presented to support the claims. 15 References. B. Annesser Murray},
	Address = {Stanford},
	Author = {Culy, Christopher},
	Booktitle = {Grammatical Relations: A Cross - Theoretical Perspective},
	Date-Modified = {2014-10-30 14:11:59 +0000},
	Editor = {Dziwirek, Katarzyna and Farrell, Patrick and Mejias - Bikandi, Errapel},
	Pages = {83--93},
	Publisher = {The Center for the Study of Language and Information, Stanford U},
	Title = {Grammatical relations and verb forms in internally headed relative clauses},
	Year = {1990}}

@incollection{Culy2002,
	Abstract = {The presence of logophoric hierarchy in discourse produced by speakers
	for whom Donno S(open o), a Dogon (Niger Congo, Gur family) dialect,
	is a first or second language, or who have only mastered its passive
	knowledge, is investigated with references to earlier studies (Culy,
	1994 & Culy et al, 1994) on the implicational hierarchy of predicates
	licensing logophoric pronouns. Evidence is found for the strong effect
	of logophoric hierarchy in this language, both within & across varieties.
	It is observed that logophoric hierarchy is not a logical necessity,
	but only a contingent fact about human language. 3 References. Z.
	Dubiel},
