@incollection{Maslova2006,
	Abstract = {The paper examines case - marking splits conditioned by focus structure.
	This type of case marking distinguishes two forms, topic/actor (unmarked)
	vs. focus/patient (marked): the latter encodes focal S & P participants,
	as well as topical Ps if A & P are not otherwise discriminated. This
	system (attested in Yukaghir & Dogon) involves a conspicuous markedness
	paradox: the least frequent constellation of parameters, A - focus,
	remains unmarked. The emergence of such systems is accounted for
	in terms of default role interpretation of NPs, guided by correlations
	between semantic & information - structure roles, whereby a focus
	marker acquires the P - marking function. If this analysis is formalized
	in terms of Bidirectional Optimality Theory, the markedness paradox
	of focus - oriented splits proves to instantiate the more general
	phenomenon of emergence of the unmarked. References. Adapted from
	the source document},
	Address = {Amsterdam},
	Author = {Maslova, Elena},
	Booktitle = {Voice and Grammatical Relations: In Honor of Masayoshi Shibatani},
	Editor = {Tsunoda, Tasaku and Kageyama, Taro},
	Pages = {175 - 194},
	Publisher = {John Benjamins},
	Title = {Information Focus in Relational Clause Structure},
	Year = {2006},
	Bdsk-File-1 = {}}

