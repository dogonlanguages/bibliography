@article{Culy1994a,
	Abstract = {Logophoricity in Donno S(open o), a Dogon language spoken in Mali
	& Burkina Faso, is examined as it relates primarily to pronouns.
	Logophoric pronouns (LPs) are defined as "referring to the person
	whose words, thoughts, or state of mind are being reported," & are
	examined for environments in which they occur. Analysis of the Donno
	S(open o) LP inyem(epsilon) shows that it is restricted in terms
	of person (third) & number (singular), but not in terms of part of
	speech, ie, it can occur as subject, direct or indirect object, oblique,
	or possessor; it can also occur in the periphrastic reflexive. Examples
	illustrate that the LP can occur anywhere in indirect discourse.
	At the sentence level, LPs in Donno S(open o) are licensed by two
	main classes of predicates (speech & thought) & by two peripheral
	constructions ("in order to" & benefactive). The interactions of
	logophoricity in Donno S(open o) with the morphological aspects of
	complementizers, verbal inflection for subject - verb agreement,
	& subject marking are each considered, & comparisons are made with
	other languages in which logophoricity occurs. 1 Table, 12 References.
	Adapted from the source document},
	Author = {Culy, Christopher},
	Date-Modified = {2014-10-30 14:13:23 +0000},
	Journal = {Journal of African Languages and Linguistics},
	Number = {2},
	Pages = {113--125},
	Title = {A Note on Logophoricity in Dogon},
	Volume = {15},
	Year = {1994}}

